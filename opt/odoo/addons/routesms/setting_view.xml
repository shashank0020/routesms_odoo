		<!-- Form view ends -->

		<!-- Search view starts -->
        <record id="view_indian_user_filter" model="ir.ui.view">
            <field name="name">ind.user.list.select</field>
            <field name="model">ind.user</field>
            <field name="arch" type="xml">
                <search string="Search Indian User">
                   <field name="username" class="oe_inline"/>

               </search>
            </field>
        </record>

		<!-- Search view ends -->
		
		<!-- Action view starts -->
        <record id="action_indian_user" model="ir.actions.act_window">
            <field name="name">Indian User</field>
            <field name="res_model">ind.user</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree,form</field>
            
            <!-- <field name="search_view_id" ref="view_vertical_business_filter"/> -->
            
